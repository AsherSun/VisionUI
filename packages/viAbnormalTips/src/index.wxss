@import '/styles/iconfont.wxss';
.noData {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  display: flex;
  flex-flow: column wrap;
  text-align: center;
  justify-content: center;
}

.noData .icon-no-data-tips {
  font-size: 55px;
}

.noData__txt {
  font-size: 12px;
  margin-top: 10px;
}

button::after {
  border-radius: 0;
  border: none;
}

button {
  border-radius: 0;
  padding:0;
  margin: 0;
  font-size: 12px;
  margin-top: 10px;
}